const User = require('../models/user')

const userController = {
  getSignIn: (req, res) => {
    return res.render('signin')
  },
  signin: (req, res) => {
    return res.redirect('/')
  },
  getSignUp: (req, res) => {
    return res.render('signup')
  },
  signout: (req, res) => {
    req.logOut(() => { })
    return res.redirect('/signin')
  }
}


module.exports = userController